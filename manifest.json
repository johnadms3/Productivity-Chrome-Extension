{
"manifest_version" : 3,
"name" : "Popup Productivity Extension",
"version" : "1.0",
"description" : "An in your face productivity/reminder extension",
"permissions" : ["alarms", "notifications", "activeTab"],
"background" : {
    "scripts" : ["background.js"],
    "persistent" : false,
    "js" : ["background.js", "extension.js"]
    },
"action": {
    "default_popup": "extension.html"
    }
}